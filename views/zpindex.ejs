<% include zpheader.ejs %>

<div style="background-color: #fff;width: 1000px;height: 680px;margin: 0 auto;">

	<table style="padding-left: 50px;padding-top: 8px;">
			<tr>
				<td class="title-theme">主题</td>
				<td class="title-type">活动类型</td>
				<td class="title-time">活动时间/截至时间</td>
				<td class="title-place">地点</td>
				<% if(loginbean==undefined){ %>
				<td class="title-collect" style="color: #AEAEAE;padding-left: 29px;">收藏</td>
				<% }else{ %>
				<td class="title-collect" style="color: #6c2393;padding-left: 29px;">收藏</td>
				<% } %>
			</tr>
	</table>
<% if(rs!=undefined){ 
	rs.forEach(function(row){ %>
	<table style="padding-left: 50px;">
			<tr>
				<td class="text-theme">
					<a class="home-title" href="/employment/detail?tid=<%= row['tid'] %>"><%= row['title'] %></a>
				</td>
				<td class="text-type"><%= row['type']%></td>
				<td class="text-time"><%= row['year']%>/<%= row['month']%>/<%= row['day']%> <%= row['hour']%>:<%= row['minute']%></td>
				<td class="text-place"><%= row['place']%></td>
				<td class="text-collect" style="padding-left: 33.5px;">
				<% if(loginbean!=undefined){ %>
					<% if(business==undefined){ %>
					<ul>
						<% if(!row['cid']){%>
						<label  for="one-box" >           
							<img  alt="初始" src="/images/Star_S_0.png" id="checkimg" onclick="window.location.href='/employment/collection?uid=<%= loginbean.id %>&tid=<%= row['tid'] %>&rank=1&page=<%=page %>&title=<%= row['title'] %>&type=<%= row['type'] %>&year=<%= row['year']%>&month=<%= row['month'] %>&day=<%= row['day'] %>&hour=<%= row['hour'] %>&minute=<%= row['minute']%>&place=<%= row['place']%>&business=<%= row['business']%>&back=0';">
						</label> 
						<% }else{ %>
						<label  for="one-box" >           
							<img  alt="初始" src="/images/Star_S_1.png" id="checkimg" onclick="window.location.href='/employment/collection/del?uid=<%= loginbean.id %>&tid=<%=row['tid']%>&page=<%=page %>&back=0'">
						</label>
						<%}%>
			
					</ul>
					<% }else{%>
					<ul>
						<% if(!row['cid']){%>
						<label  for="one-box" >           
							<img  alt="初始" src="/images/Star_S_0.png" id="checkimg" onclick="window.location.href='/employment/collection?uid=<%= loginbean.id %>&tid=<%= row['tid'] %>&rank=1&page=<%=page %>&title=<%= row['title'] %>&type=<%= row['type'] %>&year=<%= row['year']%>&month=<%= row['month'] %>&day=<%= row['day'] %>&hour=<%= row['hour'] %>&minute=<%= row['minute']%>&place=<%= row['place']%>&business=<%= row['business']%>&back=1';">
						</label> 
						<% }else{ %>
						<label  for="one-box" >           
							<img  alt="初始" src="/images/Star_S_1.png" id="checkimg" onclick="window.location.href='/employment/collection/del?uid=<%= loginbean.id %>&tid=<%=row['tid']%>&page=<%=page %>&business=<%= row['business']%>&back=1'">
						</label>
						<%}%>
					</ul>
					<% }%>
				<% }else{ %>
					<ul>
						<label  for="one-box" >           
							<img  alt="初始" src="/images/Star_S_2.png" id="checkimg" onclick="black_star()">
						</label>
					</ul>
				<% } %>
				
				</td>
			</tr>
	</table>
	<hr class="hr1">
	<% })
}%>
	
	<table style="padding-left: 50px;font-size:12px;" align="center">
		<tr >
		<%if(count!=0){%>
			<% if(business==undefined){ %>
				<td align="center" style="font-size:12px;color:#515151;padding:8px 0;">
				总共&nbsp<%=count%>&nbsp条记录&nbsp
				第&nbsp<%=page%>/<%=countPage%>&nbsp页&nbsp
				
				</td>
				<td>
				<% if (page!=1) { %>
					<a href="/?page=1">首页</a>&nbsp
					<a href="/?page=<%=(page-1)%>">上一页</a>&nbsp
				<% } 
				if (page!=countPage) { %>
				<a href="/?page=<%=(page+1)%>">下一页</a>&nbsp
				<a href="/?page=<%=countPage%>">尾页</a>&nbsp
				<% } %>
				</td>
			<% }else{%>
				<td align="center" style="font-size:12px;color:#515151;padding:8px 0;">
				总共&nbsp<%=count%>&nbsp条记录&nbsp
				第&nbsp<%=page%>/<%=countPage%>&nbsp页&nbsp
				
				</td>
				<td>
				<% if (page!=1) { %>
					<a href="/?page=1&business=<%=business%>">首页</a>&nbsp
					<a href="/?page=<%=(page-1)%>&business=<%=business%>">上一页</a>&nbsp
				<% } 
				if (page!=countPage) { %>
				<a href="/?page=<%=(page+1)%>&business=<%=business%>">下一页</a>&nbsp
				<a href="/?page=<%=countPage%>&business=<%=business%>">尾页</a>&nbsp
				<% } %>
				</td>
			<% }%>
		<%}else{%>
				<td align="center" style="font-size:12px;color:#515151">
					暂无记录
				</td>
		<%}%>
		</tr>
	</table>
	<hr class="hr1">
	 
</div>
<script src="/javascripts/alerts.js"></script>
<script src="/javascripts/no.js"></script>
<script src="http://lib.sinaapp.com/js/jquery/3.1.0/jquery-3.1.0.slim.min.js"></script>
</body>

</html>